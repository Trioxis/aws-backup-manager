    "snapshot",
    "backup"
  ],
  "author": "Trioxis",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Trioxis/aws-backup-manager/issues"
  },
  "homepage": "https://github.com/Trioxis/aws-backup-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "eslint": "^1.10.3",
    "expect.js": "^0.3.1",
    "mocha": "^2.3.4"
  }
}
