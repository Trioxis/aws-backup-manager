{
  "name": "aws-backup-manager",
  "version": "0.1.0",
  "description": "Automatically back up EBS volumes using tags",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npm run lint && node ./node_modules/mocha/bin/mocha --compilers js:babel-register",
    "lint": "node ./node_modules/eslint/bin/eslint.js index.js src/**",
    "build": "babel src -d build --source-maps",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trioxis/aws-backup-manager.git"
  },
  "keywords": [
