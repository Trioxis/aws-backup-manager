{
		"rules": {
				"indent": [
						2,
						"tab"
				],
				"quotes": [
						2,
						"single"
				],
				"semi": [
						2,
						"always"
				],
				"no-console": [
					0
				]
		},
		"env": {
				"es6": true,
				"node": true
		},
		"ecmaFeatures": {
			"modules": true
		},
		"extends": "eslint:recommended"
}
